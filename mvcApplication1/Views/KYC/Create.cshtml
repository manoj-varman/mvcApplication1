@model KYC

<div class="row">
    <div class="col-12">
        <form asp  method="post">

            <div class="form-group">
                <label >Employee Id</label>
                <input type="number" asp-for="@Model.empId" />
               </div>
            <div class="form-group">
                <label > Name</label>
                <input type="text" asp-for="@Model.empName" />
            </div>
            <div class="form-group">
                <label >Mobile </label>
                <input type="number" asp-for=" @Model.empPhone" />
            </div>
            <div class="form-group">
                <label > Role </label>
                <input type="text" asp-for="@Model.empRole" />
            </div>
            <div class="form-group">
                <label >City</label>
                <input type="text" asp-for="@Model.empCity" />
            </div>
            <button type="submit" class="btn btn-primary">Add Employee</button>

        </form>

        </div>
</div>

